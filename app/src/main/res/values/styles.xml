<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark</item>
		<item name="colorAccent">@color/colorAccent</item>
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>

	<style name="AppTheme.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>

	<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

	<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

	<style name="MyToolBar" parent="Base.Widget.AppCompat.Toolbar">
		<item name="contentInsetStart">0dp</item>
		<!--<item name="android:background">@color/colorPrimary</item>-->
	</style>


	<style name="popwindow_anim">
		<item name="android:windowEnterAnimation">@anim/pop_enter</item>
		<item name="android:windowExitAnimation">@anim/pop_exit</item>
	</style>
	<style name="popwindow_anim_left">
		<item name="android:windowEnterAnimation">@anim/pop_enter_left</item>
		<item name="android:windowExitAnimation">@anim/pop_exit_right</item>
	</style>
	<style name="dialog_anim">
		<item name="android:windowEnterAnimation">@anim/dialog_enter</item>
		<item name="android:windowExitAnimation">@anim/dialog_exit</item>
	</style>
	<style name="DialogWrap">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:background">@drawable/bg_white_radius2</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:windowFrame">@null</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowAnimationStyle">@style/dialog_anim</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:backgroundDimEnabled">true</item>
		<item name="android:backgroundDimAmount">0.6</item>
	</style>

	<style name="txt_table_title">
		<item name="android:textColor">@color/txt_color</item>
		<item name="android:gravity">center_horizontal</item>
		<item name="android:textSize">@dimen/dp16</item>
		<item name="android:padding">@dimen/dp15</item>
	</style>

	<style name="txt_table_content">
		<item name="android:textColor">@color/txt_color2</item>
		<item name="android:gravity">center_horizontal</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:textSize">@dimen/dp15</item>
		<item name="android:padding">@dimen/dp10</item>
	</style>
	<style name="MyDialog" parent="@android:Theme.Dialog">
		<item name="android:windowFrame">@null</item>
		<item name="android:windowBackground">@mipmap/none</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowContentOverlay">@null</item>
	</style>

	<style name="NativeDialog" parent="@style/MyDialog">
	</style>


